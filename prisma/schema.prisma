generator client {
    provider = "prisma-client-js"
}

datasource db {
   provider = "sqlite"
   url      = env("DATABASE_URL")
}
model Food {
    id        Int      @id @default(autoincrement())
    name    String
    description String?
    price     Decimal
    category    String
    available    Boolean @default(true)
    createdAt DateTime @default(now())

    @@map("food")
}